#include "point3d.h"

class Point3d
{
private:
  double m_x, m_y, m_z;

public:
  Point3d(double x = 0.0, double y = 0.0, double z = 0.0)
    : m_x(x), m_y(y), m_z(z)
  {

  }

  void print()
  {
    std::cout << "Point(" << m_x << ", " << m_y << ", " << m_z << ")\n";
  }

  void moveByVector(const Vector3d &v);

};
